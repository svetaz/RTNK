<resources xmlns:tools="http://schemas.android.com/tools">

    <!-- Base application theme. -->
    <style name="Theme.AppCompat.Light." parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>

        <item name="android:navigationBarColor">@color/colorPrimaryDark</item>


    </style>

    <!-- Dart theme with an action bar -->
    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar"/>

    <!-- Light theme -->
    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light"/>





    <style name="DrawerItemTextAppearance">
        <item name="android:fontFamily" tools:ignore="NewApi">sans-serif-light</item>
    </style>

    <style name="Theme.AppCompat.Light..AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar"/>

    <style name="Theme.AppCompat.Light..PopupOverlay" parent="ThemeOverlay.AppCompat.Light"/>




</resources>
